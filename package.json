{
  "name": "white-market-static",
  "description": "White Market Podcast site built with Gatsby and Netlify CMS",
  "version": "1.1.3",
  "author": "Ciaran Edwards",
  "keywords": [
    "gatsby"
  ],
  "license": "GPL-3.0",
  "main": "n/a",
  "scripts": {
    "start": "npm run develop",
    "clean": "rimraf .cache public",
    "build": "gatsby build",
    "deploy": "npm i && npm run build && netlify deploy -p -d public",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint ./*.js src --fix --ext js,ts,tsx",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix --quiet"
  },
  "dependencies": {
    "gatsby": "^2.24.58",
    "gatsby-image": "^2.4.17",
    "lodash": "^4.17.20",
    "mdi-react": "^7.3.0",
    "netlify-cms-app": "^2.12.21",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-infinite-scroller": "^1.2.4",
    "react-player": "^2.6.2",
    "react-pose": "^4.0.10",
    "styled-components": "^5.2.0",
    "typeface-crete-round": "^0.0.72",
    "typeface-montserrat": "^0.0.75",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@bundle-analyzer/gatsby-plugin": "^0.5.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-gatsby": "^0.5.9",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-testing-library": "^3.8.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.4.29",
    "gatsby-plugin-netlify": "^2.3.14",
    "gatsby-plugin-netlify-cms": "^4.3.13",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-resolve-src": "^2.1.0",
