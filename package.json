{
  "name": "white-market-static",
  "description": "White Market Podcast site built with Gatsby and Netlify CMS",
  "version": "1.1.3",
  "author": "Ciaran Edwards",
  "keywords": [
    "gatsby"
  ],
  "license": "GPL-3.0",
  "main": "n/a",
  "scripts": {
    "start": "npm run develop",
    "clean": "rimraf .cache public",
    "build": "gatsby build",
    "deploy": "npm i && npm run build && netlify deploy -p -d public",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint ./*.js src --fix --ext js,ts,tsx",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix --quiet"
  },
  "dependencies": {
    "gatsby": "^2.23.14",
    "gatsby-image": "^2.4.11",
    "gatsby-plugin-eslint": "^2.0.1",
    "gatsby-plugin-manifest": "^2.4.16",
    "gatsby-plugin-netlify": "^2.3.9",
    "gatsby-plugin-netlify-cms": "^4.3.9",
    "gatsby-plugin-react-helmet": "^3.3.8",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sass": "^2.3.9",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-plugin-sitemap": "^2.4.9",
    "gatsby-plugin-styled-components": "^3.3.8",
    "gatsby-plugin-typescript": "^2.4.10",
    "gatsby-remark-copy-linked-files": "^2.3.7",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.3.16",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.3.16",
    "gatsby-transformer-remark": "^2.8.20",
    "gatsby-transformer-sharp": "^2.5.7",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "mdi-react": "^7.3.0",
    "mp3-duration": "^1.1.0",
    "netlify-cms-app": "^2.12.15",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "pify": "^5.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.10.0",
    "react-infinite-scroller": "^1.2.1",
    "react-player": "^2.4.0",
    "react-pose": "^4.0.10",
    "rss": "^1.2.2",
    "styled-components": "^5.1.1",
    "typeface-crete-round": "^0.0.72",
    "typeface-montserrat": "^0.0.75",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.4.12",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-testing-library": "^3.3.1",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "netlify-cli": "^2.57.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  }
}
